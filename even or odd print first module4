#include <stdio.h>
int main() 
{
    int a,b,c[10];
    scanf("%d%d",&a,&b);
    int i=0;
    while(b>0)
    {
    c[i]=b%10;
    b=b/10;
    i++;
    }
    if(a==0)
    {
    for(int j=i-1;j>=0;j--)
    {
        if(c[j]%2!=0)
        {
            printf("%d",c[j]);
        }
    }
    for(int j=i-1;j>=0;j--)
    {
        if(c[j]%2==0)
        {
            printf("%d",c[j]);
        }
    }
    }
    else
    {
    for(int j=i-1;j>=0;j--)
    {
        if(c[j]%2==0)
        {
            printf("%d",c[j]);
        }
    }
    for(int j=i-1;j>=0;j--)
    {
        if(c[j]%2!=0)
        {
            printf("%d",c[j]);
        }
    }
    }
	return 0;
}
