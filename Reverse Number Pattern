#include <stdio.h>
int main() 
{
    int n,i,j;
    scanf("%d",&n);
    for(i=5;i>=1;i--)
    {
        for(j=n;j>=i;j--)
        {
            printf("%d ",j);    
        }
    printf("\n");
    }
    return 0;
}

I/P:
5
O/P:
5 
5 4 
5 4 3 
5 4 3 2 
5 4 3 2 1 




#include <stdio.h>
int main() 
{
    int n,i,j;
    scanf("%d",&n);
    for(i=5;i>=1;i--)
    {
        for(j=n;j>=i;j--)
        {
            printf("%d ",i);    
        }
    printf("\n");
    }
    return 0;
}


I/P:
5
O/P:
5 
4 4 
3 3 3 
2 2 2 2 
1 1 1 1 1 
