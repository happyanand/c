#include <stdio.h>
struct student
{
    int id;
    char *name;
    int m1,m2;
};
void main() 
{
    int i=0;
    struct student s[2];
    s[0].id=06;
    s[0].name="ANAND";
    s[0].m1=90;
    s[0].m2=80;
    s[1].id=10;
    s[1].name="KUMAR";
    s[1].m1=70;
    s[1].m2=30;
    for(i=0;i<2;i++)
    {
    printf("%d \n %s \n %d \n",s[i].id,s[i].name,(s[i].m1+s[i].m2));
    }
}

O/P:
6 
 ANAND 
 170 
10 
 KUMAR 
 100 
