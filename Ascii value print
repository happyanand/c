#include <stdio.h>
int main() 
{
    int n;
    scanf("%d",&n);
    for(int i=97;i<=n+97;i++)
    {
        for(int j=97;j<=n+97;j++)
        {
            if(i>=j)
            printf("%c",j);
        }
        printf("\n");
    }
    return 0;
}



